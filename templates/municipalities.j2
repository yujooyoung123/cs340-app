<html>
<head>
<title>Municipalities</title>
<link href="style.css" rel="stylesheet" type="text/css" />
</head>

<body>
<h1>Municipalities</h1>

    [<a href="/">Home</a> | <a href="projects">Projects</a> | <a href="municipalities">Municipalities</a> |
     <a href="owners">Owners</a> | <a href="cases">Cases</a> |
     <a href="lawyers">Lawyers</a> ]
<div id="browse" style="display: block">

<p><strong>Browse Municipalities</strong></p>

<table>
  <thead>
  <table border="1" cellpadding="5">
  <tr>
      <!-- Iterate through each key in the first entry to get the
      column name -->
      {% for key in Data[0].keys() %}

      <!-- Create a <th> tag with the key inside of it, this will be
      our header row -->
      <th>{{ key }}</th>

      <!-- End of this for loop -->
      {% endfor %}
      <td>  </td>
      <td>  </td>
  </tr>
  <tbody>

  <!-- Now, iterate through every person in Projects -->
  {% for municipality in Data %}
  <tr>
      <!-- Then iterate through every key in the current person dictionary -->
      {% for key in municipality.keys() %}
      <!-- Create a <td> element with the value of that key in it -->
      <td>{{municipality[key]}}</td>
      {% endfor %}
      <td><a href="update-municipality/{{ municipality["municipalityID"] }}">Update</a></td>
      <td><a href="delete-municipality/{{ municipality["municipalityID"] }}">Delete</a></td>
  </tr>
  {% endfor %}
</table>

<p>&nbsp;</p>
</div> <!-- browse -->

<!-- <div>
  <a>Search for a Project </a>
  <input type="text" placeholder="Enter Project ID">
</div> -->

<div id="insert" style="display: block">
    <form method="POST" action="add-municipality">
        <legend><strong>Add Municipality</strong></legend>
          <fieldset class="fields">
            <label> Municipality ID </label> <input type="number" name="municipalityID" required>
            <label> Municipality Name </label> <input type="text" name="municipalityName" required>
            <label> Website </label> <input type="text" name="website" required>
            <label> State Name </label> <input type="text" name="stateName" required>
            <label> File Location </label> <input type="text" name="fileLocation" required>
          </fieldset>
          <input class="btn" type="submit" id="add-municipality" value="Add a Municipality">
	  </form> <!--add new project-->
</div>
</body>
</html>